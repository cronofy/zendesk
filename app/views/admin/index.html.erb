<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>Cronofy ID</th>
      <th>Zendesk Subdomain</th>
      <th>Zendesk ID</th>
      <th>Created</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= user.cronofy_id %> <% if user.cronofy_credentials? %><span class="glyphicon glyphicon-ok"></span><% end %></td>
      <td><%= user.zendesk_subdomain %></td>
      <td><%= user.zendesk_user_id %> <% if user.zendesk_credentials? %><span class="glyphicon glyphicon-ok"></span><% end %></td>
      <td><%= user.created_at %></td>
      <td><%= button_to "Delete", admin_delete_account_path(user_id: user.id), data: { confirm: "Are you sure?" } %></td>
    </tr>
  <% end %>
  </tbody>
</table>